{
    "name": "andrewgjohnson/imagettftextblur",
    "type": "library",
    "description": "imagettftextblur is a drop in replacement for imagettftext with added parameters to add blur, glow and shadow effects to your PHP GD images",
    "keywords": [
        "gd",
        "graphics",
        "image",
        "images"
    ],
    "homepage": "https://imagettftextblur.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew G Johnson",
            "email": "andrew@andrewgjohnson.com",
            "homepage": "http://www.andrewgjohnson.com/",
            "role": "Project Lead"
        },
        {
            "name": "Philip van Heemstra",
            "homepage": "https://github.com/vHeemstra",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": ">=5.0.0",
        "ext-gd": "*"
    },
    "autoload": {
        "files": [
            "source/imagettftextblur.php"
        ]
    }
}
